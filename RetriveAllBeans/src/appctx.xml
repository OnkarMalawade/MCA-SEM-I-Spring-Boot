<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student1" class="myPack.Student">
        <property name="rollNo" value="1" />
        <property name="studNm" value="ABC" />
    </bean>
	
	<bean id="student2" class="myPack.Student">
        <property name="rollNo" value="2" />
        <property name="studNm" value="PQR" />
    </bean>
    
    <bean id="student3" class="myPack.Student">
        <constructor-arg name="rollNo" value="3"></constructor-arg>
        <constructor-arg name="studNm" value="XYZ"></constructor-arg>
    </bean>
</beans>